{
	"manifest_version": 2,

	"name": "Tomato",
	"description": "Tomato Timer",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Not an apple.",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["moment.js", "background.js"],
		"persistent": false
	}, 
	"permissions": 	["notifications"]
}